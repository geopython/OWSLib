            <Output>
                <ows:Identifier>boolean</ows:Identifier>
                <ows:Title>Boolean</ows:Title>
                <ows:Abstract></ows:Abstract>
                <LiteralOutput>
                <ows:DataType ows:reference="http://www.w3.org/TR/xmlschema-2/#boolean">boolean</ows:DataType>
